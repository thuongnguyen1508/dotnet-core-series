    "Series.DocumentDB": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "MONGGO_DB_CONNECTION_STRING": "mongodb://root:rootpassword@localhost:27017",
        "DB_VERSION": "1"
      }
    }
  }
